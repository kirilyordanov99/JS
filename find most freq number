let arr=[1, 5, 1, 3, 5, 5, 2];
let mostfreq = 1;
let m = 0;
let item;
for (let i=0; i<arr.length; i++)
{
        for (let j=i; j<arr.length; j++)
        {
                if (arr[i] == arr[j])
                 m++;
                if (mostfreq<m)
                {
                    mostfreq=m; 
                  item = arr[i];
                }
        }
        m=0;
}
console.log(item);